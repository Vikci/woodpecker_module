{
  "name": "vicki/woodpecker_module",
  "description": "OpenEMR Custom Module Woodpecker Starter Project",
  "type": "openemr-module",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Vicki",
      "email": "wzang@altatech.dev",
      "role": "Developer"
    }
  ],
  "config": {
    "disable-scripts": false
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/Vikci/woodpecker_module.git"
    }
  ],
  "keywords": ["openemr", "openemr-module"],
  "autoload": {
    "psr-4": {"OpenEMR\\Modules\\CustomModuleWpecker\\": "src/"}
  },
  "require": {
    "composer/installers": "^2.0",
    "php": ">= 7.4"
  },
  "extra": {
    "installer-paths": {
      "interface/modules/custom_modules/woodpecker_module": ["vicki/woodpecker_module"]
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@php -r \"if (!file_exists('interface/modules/custom_modules/woodpecker_module')) { mkdir('interface/modules/custom_modules/woodpecker_module', 0755, true); }\"",
      "@php -r \"if (file_exists('vendor/vicki/woodpecker_module')) { rename('vendor/vicki/woodpecker_module', 'interface/modules/custom_modules/woodpecker_module'); }\"",
      "composer dump-autoload"
    ],
    "post-update-cmd": [
      "@php -r \"if (!file_exists('interface/modules/custom_modules/woodpecker_module')) { mkdir('interface/modules/custom_modules/woodpecker_module', 0755, true); }\"",
      "@php -r \"if (file_exists('vendor/vicki/woodpecker_module')) { rename('vendor/vicki/woodpecker_module', 'interface/modules/custom_modules/woodpecker_module'); }\"",
      "composer dump-autoload"
    ]
  },
  "conflict": {
    "openemr/openemr": "<6.0.0"
  }
}
